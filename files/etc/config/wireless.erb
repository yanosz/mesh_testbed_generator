config wifi-device 'radio0'
        option type 'mac80211'
        option hwmode '11g'
        option path 'platform/qca953x_wmac'
        option htmode 'HT20'
        option country 'DE'
        option legacy_rates '0'
        option txpower '18'
        option channel '<%= @node['channel']%>'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option mode 'ap'
        option encryption 'none'
        option ssid 'Freifunk'
        option network 'lan'

config wifi-iface
        option device 'radio0'
        option mode 'ap'
        option encryption 'none'
        option ssid '<%= @node['misc_ssid']%>'
        option network 'lan'
